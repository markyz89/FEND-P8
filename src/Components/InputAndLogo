

this.state = {
			displaySidebar: false
		}



 this.sidebarHandler = this.sidebarHandler.bind(this)

			sidebarHandler() {
  let displaySidebar = this.state.displaySidebar
  this.setState({
    displaySidebar: !displaySidebar
  })
}


    const popOutSideBar = {
    left: '25%'
    }

    const sidebar = this.state.displaySidebar

    let showSidebar = {
      if (sidebar) {
        showSidebar = popOutSideBar
      }
    }

     style={showSidebar}